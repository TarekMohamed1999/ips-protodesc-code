HeaderExt := {
	id      : Bit16;
	len     : Bit16;
	payload : Bit[];
} where {
	payload.length == 32*len;
};

RTPPacket := {
	version          : Bit2;
	padding_flag     : Bit;
	extension        : Bit;
	csrc_count       : Bit4;
	marker           : Bit;
	payload_type     : Bit7;
	sequence_num     : Bit16;
	timestamp        : Bit32;
	ssrc_id          : Bit32;
	csrc_list        : Bit[];
	header_extension : HeaderExt;
	payload          : Bit[];
	padding          : Bit[];
	padding_count    : Bit8;
} where {
	version == 2;
	csrc_list.length == 32*4*crsc_count;
	padding.length == ((padding_count) ? 8*padding_count : 0);
};

PDUs := {RTPPacket};