header_ext := {
	id     : bit[16];
	len    : bit[16];
	payload : bit[];
} where {
	payload.length == 32*len;
};

rtp_packet := {
	version    : bit[2];
	padding    : bit;
	extension  : bit;
	csrc_count : bit[4];
	marker     : bit;
	payload_type : bit[7];
	sequence_num : bit[16];
	timestamp : bit[32];
	ssrc_id   : bit[32];
	csrc_list : bit[];
	header_extension : header_ext;
	payload : bit[];
	padding : bit[];
	padding_count : bit[8];
} where {
	version == 2;
	csrc_list.length == 32*4*crsc_count;
	header_extension.is_present = (extension == 1);
	padding_count.is_present = (padding == 1);
	padding.length == 8*padding_count;
};

pdus := {rtp_packet};